listener "tcp" {
  address          = "0.0.0.0:8200"
  cluster_address  = "{{ ansible_eth1.ipv4.address }}:8201"
  tls_disable      = "true"
}

storage "consul" {
  address = "{{ ansible_eth1.ipv4.address }}:8500"
  path    = "vault/"
}

api_addr = "http://{{ ansible_eth1.ipv4.address }}:8200"
cluster_addr = "http://{{ ansible_eth1.ipv4.address }}:8201"
